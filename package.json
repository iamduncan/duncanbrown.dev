{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "pnpm generate:css --minify",
    "build:remix": "remix build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "pnpm generate:css --watch",
    "dev:remix": "netlify dev",
    "generate:css": "pnpm tailwindcss -i ./styles/tailwind.css -o ./app/styles/tailwind.css",
    "start": "cross-env NODE_ENV=production netlify dev"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.12",
    "@netlify/functions": "^1.3.0",
    "@remix-run/netlify": "^1.7.3",
    "@remix-run/node": "^1.7.3",
    "@remix-run/react": "^1.7.3",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "gray-matter": "^4.0.3",
    "hastscript": "^7.1.0",
    "isbot": "^3.6.1",
    "mdx-bundler": "^9.0.1",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.0.1",
    "rehype-stringify": "^9.0.3",
    "remark-parse": "^10.0.1",
    "remark-prism": "^1.3.6",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.3",
    "@remix-run/eslint-config": "^1.7.3",
    "@remix-run/serve": "^1.7.3",
    "@tailwindcss/typography": "^0.5.7",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/remark-prism": "^1.3.3",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "rehype-add-classes": "^1.0.0",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
